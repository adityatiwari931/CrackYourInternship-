class Solution {
    public void rotate(int[][] matrix) {
        int temp;
        for(int i =0;i<matrix.length;i++)
        {
            for(int j =i+1;j<matrix[0].length;j++)
            {
               if(i!=j)
               {
                   temp=matrix[i][j];
                   matrix[i][j]=matrix[j][i];
                   matrix[j][i]=temp;
               } 
            }
        }
        temp=0;
        for(int i = 0; i < matrix.length; i++)
        {
            int l = 0;
            int h = matrix.length-1;
            while(l < h)
            {
                temp = matrix[i][l];
                matrix[i][l] = matrix[i][h];
                matrix[i][h] = temp;
                l++;
                h--;
            }
        }
    }
}
